{
  "manifest_version": 3,
  "name": "Contador de O.S",
  "version": "2.0",
  "description": "Conta as O.S do dia",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "https://ixc.vemprauno.com.br/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://ixc.vemprauno.com.br/*"
      ],
      "js": [
        "content.js"
      ],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}